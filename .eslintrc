{
    "extends": "airbnb-base",
    "env": {
        "browser": true
    },
    "rules": {
        "linebreak-style": 0,
        "global-require": 0,
        "eslint linebreak-style": [0, "error", "windows"],
        "no-use-before-define": 0,
        "no-shadow": 0,
        "arrow-parens": 0,
        "no-plusplus": 0,
        "no-return-assign": 0,
        "no-unused-vars": ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
        "no-return-await": 0,
        "no-param-reassign": 0,
        "max-len": 0,
        "implicit-arrow-linebreak": 0,
        "comma-dangle": 0,
        "function-paren-newline": 0,
        "object-curly-newline": 0
    }
}

